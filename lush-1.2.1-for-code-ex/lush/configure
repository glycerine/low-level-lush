/* Thanks to Paul Eggert for this test.  */
#include <stdio.h>
#include <stdlib.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <sys/wait.h>
#if HAVE_UNISTD_H
# include <unistd.h>
